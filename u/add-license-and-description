sub ensure-authors-correct-in-toml($file, @authors) {

    say "ensuring authors correct in toml for $file";

    my %toml = from-toml($file.IO.slurp);
    %toml<package><authors>               = [ "klebs <none>" ];
    %toml<package><description>           = "todo";
    %toml<package><license>               = "BSD-3-Clause";

    #why does this come in with quotes on the key? 
    
    #(fixed with a manual step after generation)
    
    %toml<package><metadata><cpp-authors> = @authors;

    $file.IO.spurt: to-toml(%toml);
}

race for @caffe2-tomls {
    ensure-authors-correct-in-toml($_, @caffe2-authors);
}

race for @aten-tomls {
    ensure-authors-correct-in-toml($_, @aten-authors);
}

race for @c10-tomls {
    ensure-authors-correct-in-toml($_, @c10-authors);
}
